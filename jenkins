pipeline {
    agent any

    stages {
        stage('CHECKOUT') {
            steps {
                echo 'checkout is successfully completed'
            }
            }
        stage('BUILD') {
            steps {
                echo 'build is successfully completed'
             }
        }
        stage('TEST') {
            steps {
                echo 'test is successfull'
           }
       }
        stage('DEPLOYMENT') {
            steps {
                echo 'DEPLOYMENT IS SUCCESSFULL'
           }
        }
    }
}
