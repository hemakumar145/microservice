                echo 'build is successfully completed'
             }
        }
        stage('TEST') {
            steps {
                echo 'test is successfull'
           }
       }
        stage('DEPLOYMENT') {
            steps {
                echo 'DEPLOYMENT IS SUCCESSFULL'
           }
        }
    }
}
