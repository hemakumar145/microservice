pipeline {
  agent any
    stages {
         stage ( "BUILD STAGE" ) {
           steps {
             sh "mvn clean install" 
            echo "the build is execuated"
              }
        }
          stage ( 'TEST STAGE' )  {
            steps {
              sh "mvn test"
              echo "the test is execuated"
        }
    } 
  }
}



